{
    "name": "zenstruck/elastica-bundle",
    "description": "Lightweight Symfony bundle wrapping Elastica.",
    "keywords": ["search", "elastica"],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com",
            "homepage": "http://zenstruck.com"
        }
    ],
    "require": {
        "ruflin/elastica": "^1.4.2|~2.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/http-kernel": "~2.3|~3.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.4",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\ElasticaBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\ElasticaBundle\\Tests\\": "tests/" }
    }
}
